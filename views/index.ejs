.category .box-container{
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(16rem, 1fr));
    gap: 2rem;
}
.category .box-container .box{
    text-align: center;
}
.category .box-container .box:hover h3{
    color: #fed2d1;
}
/* Default animation for slow pulsing zoom */
.category .box-container .box img {
    height: 6rem; /* Smaller than the original 9rem */
    width: 13rem;
    margin-bottom: 1rem;
    animation: zoomPulse 2.5s ease-in-out infinite;
    transition: transform 0.4s ease;
}
                      
  
  /* On hover: pause animation and apply zoom-out */
  .category .box-container .box:hover img {
    animation-play-state: paused;
    transform: scale(1); /* Zoom out slightly */
  }
  
  /* Keyframes for pulsing animation */
  @keyframes zoomPulse {
    0%, 100% {
      transform: scale(1);
    }
    50% {
      transform: scale(1.05);
    }
  }
  
  .category .box-container .box {
    text-align: center;
    transition: transform 0.4s ease;
  }
  
  .category .box-container .box:hover {
    transform: translateY(-3px);
  }
    
  
.category .box-container .box h3 {
    font-size: 1.7rem;
    color: #222;
    font-weight: bold;
    font-family: 'Poppins', sans-serif; /* Match this to your header's font */
}

/* ========== GENERAL STYLES ========== */
.top {
    font-size: 3rem;
    color: gray;
    text-align: left;
    font-family: "Poppins", "Segoe UI", "Helvetica Neue", sans-serif;
    margin-left: 5%;  /* controls the space from the left */
    margin-bottom: 2rem;
    
}


img {
    width: 100%;
    display: block;
}

.container {
    width: 88vw;
    margin: 0 auto;
}

.lg-title,
.md-title,
.sm-title {
   font-family: "Poppins", "Segoe UI", "Helvetica Neue", sans-serif;
    padding: 0.6rem 0;
    text-transform: capitalize;
}

.lg-title {
    font-size: 2.5rem;
    font-weight: 500;
    text-align: center;
    padding: 1.3rem 0;
    opacity: 0.9;
}

.md-title {
    font-size: 2rem;
}

.sm-title {
    font-weight: 300;
    font-size: 1rem;
    text-transform: uppercase;
}

.text-light {
    font-size: 1rem;
    font-weight: 600;
    line-height: 1.5;
    opacity: 0.5;
    margin: 0.4rem 0;
}

/* ========== PRODUCTS SECTION ========== */
.products {
    background: var(--alice-blue);
    padding: 3.2rem 0;
}

.products .text-light {
    text-align: center;
    width: 70%;
    margin: 0.9rem auto;
}

.product {
    margin: 2rem;
    position: relative;
}

.product-content {
    background: var(--gray);
    padding: 3rem 0.5rem 2rem 0.5rem;
    cursor: pointer;
}

.product-img {
    background: var(--white-light);
    box-shadow: 0 0 20px 10px var(--white-light);
    width: 150px !important;
    height: 150px !important;
    margin: auto;
    border-radius: 50%;
    transition: 0.5s ease;
}

.product-img img {
    transition: transform 0.6s ease;
}

.product:hover .product-img img {
    transform: scale(1.1);
}

.product:hover .product-img {
    background: var(--carribean-pink);
}

.product-btns {
    display: flex;
    justify-content: center;
    margin-top: 1.4rem;
    opacity: 0;
    transition: opacity 0.6s ease;
}

.product:hover .product-btns {
    opacity: 1;
}

.btn-cart,
.btn-buy {
    background: #929191;
    padding: 0.8rem 0;
    width: 125px;
    font-family: inherit;
    text-transform: uppercase;
    cursor: pointer;
    border: none;
    transition: all 0.6s ease;
}

.btn-cart:hover,
.btn-buy:hover {
    background: var(--carribean-pink);
    color: black;
}

.product-info {
    background: #f8dbda;
    padding: 15px;
    text-align: center;
}

.product-name {
    color: black;
    display: block;
    text-decoration: none;
    font-size: 1rem;
    text-transform: uppercase;
    font-weight: bold;
    text-align: center;
}

.product-price {
    font-size: 0.9rem;
    padding-top: 0.5rem;
    color: black;
}

.slider {
    width: 960px; 
    margin: 0 auto;
    overflow-x: auto;
    scroll-snap-type: x mandatory;
    -webkit-overflow-scrolling: touch;
}

.slider-track {
    display: flex;
    gap: 1rem;
    width: fit-content;
}

.slider .product {
    scroll-snap-align: center;
    flex: 0 0 220px;
    max-width: 220px;
    transition: transform 0.3s ease;
}

/* ========== RESPONSIVE PRODUCT GRID ========== */
.product-container {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
    gap: 20px;
}

@media screen and (min-width: 992px) {
    .product-items,
    .product-col-r-bottom {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
    }
}

@media screen and (min-width: 1200px) {
    .product-items {
        grid-template-columns: repeat(3, 1fr);
    }

    .product {
        margin: 0 1rem;
    }

    .products .text-light {
        width: 50%;
    }
}

@media screen and (min-width: 1336px) {
    .product-items {
        grid-template-columns: repeat(4, 1fr);
    }

    .product-collection-wrapper {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
    }

    .flex {
        height: 60vh;
    }

    .product-col-left {
        height: 121.5vh;
    }
}


