  .home {
  position: relative;
  padding: 0;
  margin-top: 80px; 
}

  .home .slide {
    display: none;
    padding: 21rem;
    min-height: 100vh;
    background-size: cover;
    background-position: center;
    opacity: 0;
    transition: opacity 1s ease-in-out;
    position: absolute;
    width: 100%;
    top: 0;
    left: 0;
  }
  
  .home .slide.active {
    display: flex;
    opacity: 1;
    position: relative;
    z-index: 1;
    animation: fadeIn 1s ease-in-out;
  }
  
  @keyframes fadeIn {
    from {
      opacity: 0;
      transform: scale(1.02);
    }
    to {
      opacity: 1;
      transform: scale(1);
    }
  }
  
  .home .slide.content{
    margin: 60px ;
    text-align: left center;
    padding-top: 18rem; 
  }
 
  .home .slide .content span{
    color: #fff;
    display: block;
    text-align: left;
    font-size: 5rem;
     text-shadow: 0 0.5rem 1rem rgba(0, 0, 0,0.7);
     animation: fadeIn .2s linear backwards;
     text-transform: uppercase;
     font-family: "Poppins", "Segoe UI", "Helvetica Neue", sans-serif;
  }
  .home .slide .content span:hover{
    color: #fed2d1;
  }
.home .slide .content h3{
    font-size: 1.5rem;
    color: #fff;
    font-family: "Poppins", "Segoe UI", "Helvetica Neue", sans-serif;
    text-shadow: 0 0.5rem 1rem rgba(0, 0, 0,0.2);
    text-shadow: fadeIn .2s linear backwards .4s;
}
.home.slide.content .btn{
    animation: fadeIn .2s linear backwards .6s;
}
@keyframes slideInLeft {
    0% {
      opacity: 0;
      transform: translateX(-100%);
    }
    100% {
      opacity: 1;
      transform: translateX(0);
    }
  }
  .home .slide .content span,
.home .slide .content h3 {
  transition: transform 0.4s ease, color 0.4s ease;
}

.home .slide .content span:hover,
.home .slide .content h3:hover {
  transform: scale(1.1); /* slightly zoom out */
  color: #fed2d1; /* optional: color change on hover */
}
.home .slide .btn {
    transition: transform 0.3s ease;
  }
  
  .home .slide .btn:hover {
    transform: scale(0.95);
  }
  

  
  .home .slide.active .content {
    animation: slideInLeft 1s ease-out forwards;
  }
  
